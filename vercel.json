{
    "installCommand": "npm install && cd client && npm install",
    "buildCommand": "cd client && npm run build",
    "outputDirectory": "client/dist",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/$1"
        }
    ],
    "cleanUrls": true,
    "functions": {
        "api/[...route].ts": {
            "maxDuration": 60
        }
    }
}